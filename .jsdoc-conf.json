{
    "source": {
        "includePattern": "\\.ts$",
        "excludePattern": "\\.test\\.ts$"
    },
    "sourceType": "module",
    "recurseDepth": 10,
    "tags": {
        "allowUnknownTags": true
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/better-docs/typescript"
    ],
    "opts": {
        "systemName": "NHAI",
        "copyright": "Copyright 2022, Adam Powers",
        "template": "node_modules/better-docs",
        "readme": "README.md",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    }
}
